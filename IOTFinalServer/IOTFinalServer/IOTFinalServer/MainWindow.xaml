<Window x:Class="IOTFinalServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IOTFinalServer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="343" Margin="13,66,0,0" VerticalAlignment="Top" Width="758" ItemsSource="{Binding ServiceList}" AutoGenerateColumns="False"></DataGrid>
        <Button Content="Unfinished service" HorizontalAlignment="Left" Margin="12,13,0,0" VerticalAlignment="Top" Width="101" Height="33"/>
        <Button Content="Finished service" HorizontalAlignment="Left" Margin="136,13,0,0" VerticalAlignment="Top" Width="107" Height="33"/>
        <Button Content="Open Position window" HorizontalAlignment="Left" Margin="602,13,0,0" VerticalAlignment="Top" Width="169" Height="33"/>
        <Button Content="Open Order window" HorizontalAlignment="Left" Margin="450,13,0,0" VerticalAlignment="Top" Width="127" Height="33" Background="#FFD2F0F7"/>
        
    </Grid>
</Window>
